{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "REACT_APP_API_URL": "https://graceful-strength-production-360f.up.railway.app",
      "REACT_APP_IMAGE_URL": "https://graceful-strength-production-360f.up.railway.app/images"
    }
  }